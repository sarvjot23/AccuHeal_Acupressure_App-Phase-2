# AccuHeal Production Environment Variables
# Copy this file to .env and fill in your actual values

# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=accuheal-prod.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=accuheal-prod
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=accuheal-prod.firebasestorage.app
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_firebase_app_id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Algolia Configuration
EXPO_PUBLIC_ALGOLIA_APP_ID=your_algolia_app_id
EXPO_PUBLIC_ALGOLIA_SEARCH_KEY=your_algolia_search_only_api_key

# App Configuration
EXPO_PUBLIC_APP_ENV=production
EXPO_PUBLIC_APP_VERSION=1.0.0

# EAS Build Configuration
EAS_PROJECT_ID=accuheal-prod-eas-id